<!DOCTYPE html>
<html>
<head>
    <title>Shoutout Leaders</title>
    <style>
        body {
            color: #FFD100;
        }
    </style>
</head>
<body>
    <div class="container">
    <h2>Shoutout Leaders</h2>
    
    <ol>
        <% results.forEach(function(r) { %>
            <li><%= r.name %> - <%= r.shoutouts %></li>
        <% }); %>
    </ul>
    
    </div>
</body>
</html>